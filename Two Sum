class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        index = []
        flag = False

        for i in range(len(nums)):
            temp1 = target - nums[i]
            for j in range(i+1, len(nums)):
                if temp1 == nums[j]:
                    index.append(i)
                    index.append(j)
                    flag = True
                    break
            if flag:
                break
        return index

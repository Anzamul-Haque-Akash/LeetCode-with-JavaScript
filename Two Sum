
var twoSum = function(nums, target) {
    
    var flag = false;
    var ans = [];
    
    for (var i = 0; i<nums.length; i++){
        var temp = target - nums[i];
        
        for (var j = 0; j<nums.length; i++){
            if(nums[i] == temp){
                flag = true;
                ans[0] = i;
                ans[1] = j;
                break;
            }
        }
        
        if(flag == true){
            break;
        }
    }
